public class Driver {
    public static void main(String[]args){
	stringStuff s = new stringStuff();
	System.out.println(s.diff21(14));
	System.out.println(s.makeAbba("OMG", "HI"));
	System.out.println(s.nonStart("Hello", "there"));
	System.out.println(s.nearHundred(91));
	System.out.println(s.mixStart("1ix snacks"));
	System.out.println(s.teaParty(20, 8));
	System.out.println(s.lastDigit(12, 13, 22));
    }
}
